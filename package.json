    "clsx": "^2.1.1",
    "isbot": "^5.1.17",
    "lodash": "^4.17.21",
    "lucide-react": "^0.454.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tiny-invariant": "^1.3.3",
    "shadcn": "^2.1.4",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
